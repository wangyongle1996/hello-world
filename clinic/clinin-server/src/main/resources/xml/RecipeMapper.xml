        <result column="stauts" property="stauts" />
        <result column="type" property="type" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, diagnose, advice, drug_recipe_id, price, stauts, type
    </sql>

</mapper>
